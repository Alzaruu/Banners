<Window x:Class="Banners5.VseWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Banners5"
        mc:Ignorable="d"
        Title="VseWin" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="52*"/>
            <RowDefinition Height="165*"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Button x:Name="Check" Content="Чек" Grid.Column="0" FontSize="30" Click="Check_Click"/>
            <Button x:Name="checkInf" Content="Инф о чеке" Grid.Column="1" FontSize="30" Click="checkInf_Click"/>
            <Button x:Name="emp" Content="Заказчики" Grid.Column="2" FontSize="30" Click="emp_Click"/>
        </Grid>
        <Frame x:Name="forPag" Grid.Row="1"/>
    </Grid>
</Window>
